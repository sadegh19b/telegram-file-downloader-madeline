# Telegram API Credentials
API_ID=your_api_id
API_HASH=your_api_hash

# File Storage Configuration
UPLOAD_DIR=uploads
BASE_URL=https://your-domain.com/uploads/

# Optional Settings
MAX_FILE_SIZE=100000000  # 100MB in bytes
ALLOWED_MIME_TYPES=*     # Use * for all types or specify comma-separated values

# Logger Settings
LOGGER_USERNAME=FileDownloader  # Username to show in logs 